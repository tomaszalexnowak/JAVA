
public class testowanie {

	public static void main(String[] args) {
		
		String a = "test test  test test ";
		
		char spr = ' ';
		int suma=0;
		for(int i = 0; i < a.length(); i++)
		{
			if(spr == a.charAt(i))
			{
				suma = suma + Integer.parseInt(a.charAt(i) + "");
			}
		}
		System.out.print(suma);

	}

}
